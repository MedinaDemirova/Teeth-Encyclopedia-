
<div class="table-basket">
    <table>
        <tr>
            <th>Product ID</th>
            <th>Quantity</th>
            <th>Status</th>
        </tr>

        {{#each products}}
        <tr>
            <td>{{_id}}</td>
            <td>{{quantity}}</td>
            <td><a href="/e-shop/order/{{orderID}}/complete-order">Complete</a></td>
        </tr>
        {{/each}}
    </table>

</div>
<a href="/e-shop/order/show-orders"><button class="confirm btn btn-info">Back</button></a>